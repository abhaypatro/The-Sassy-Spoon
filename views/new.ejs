<%- include('partials/header'); %>
<div class="container text-center">
    <h2>Add new item</h2>
    <form action="/menu" method="POST">
        <div class="form-group">
            <input class="form-control" type="text" placeholder="add name" name="name">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" placeholder="add image url" name="image">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" placeholder="price" name="price">
        </div>
        <div class="form-group">
            <button class="form-control btn btn-primary inline">ADD</button>
        </div>
    </form>
</div>
<%- include('partials/footer'); %>